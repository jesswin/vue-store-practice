MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,UAAA,MAAA;AAAA,aAAE,QAAA,CAAA,SAAA,EAAF;AAAA,KAAA,CAKP;AAJE,IAAA,QAAQ,EAAE,QAAA,CAAA,UAIZ;AAHC,IAAA,IAAI,EAAC,GAGN;AAFC,aAAM;AAEP,GAND,E,iBAKM,QAAA,CAAA,UAAA,GAAU,OAAV,GAAU,a,CALhB,EAKgB;AAAA;AALhB,IAKgB,CAAA,UAAA,CALhB,CAOI,CAhBN,CAiBI,CAnBN,CAoBI,CArBN,C","sourcesContent":["<template>\n  <div class=\"col mb-4\">\n    <div class=\"card\">\n      <img :src=\"product.url\" class=\"card-img-top\" alt=\"...\" />\n      <div class=\"card-body\">\n        <h5 class=\"card-title\">{{ product.name }}</h5>\n        <p class=\"card-text\">\n          ${{ product.price }}\n          <br />\n          <small>\n            {{ product.shortdesc }}\n          </small>\n        </p>\n        <a\n          @click=\"addToCart()\"\n          :disabled=\"itemInCart\"\n          href=\"#\"\n          class=\"btn btn-primary\"\n          >{{ itemInCart ? \"Added\" : \"Add to Cart\" }}</a\n        >\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { mapActions, mapGetters } from \"vuex\";\nexport default {\n  name: \"Product\",\n  props: [\"product\"],\n  methods: {\n    ...mapActions([\"addCartItem\"]),\n    addToCart() {\n      if (!this.itemInCart) {\n        this.addCartItem(this.product);\n      } else {\n        alert(\"Item already added to Cart\");\n      }\n    },\n  },\n  computed: {\n    ...mapGetters([\"getCartData\"]),\n    itemInCart() {\n      let inCart = false;\n      this.getCartData.forEach((item) => {\n        if (item.id == this.product.id) {\n          inCart = true;\n        }\n      });\n\n      return inCart;\n    },\n  },\n};\n</script>\n"],"sourceRoot":""}]}